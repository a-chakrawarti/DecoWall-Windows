.QLabel {
    font-size: 11.7 px;
    font-weight: bold;
    color: grey;
    }

.QPushButton {
    font-size: 11.5px;
    color: grey;
    border: 1px solid #828790;
    border-radius: 5px;
    }

.QPushButton#timeBtn {
    color: grey;
    border-top-left-radius: 0px ;
    border-bottom-left-radius: 0px;
    border-top-right-radius: 5px ;
    border-bottom-right-radius: 5px;
    border: 1px solid #828790;
    background: #e1e1e1;
    margin-left: 3px;
    background-color: #2a2a2e;
    }

.QWidget {
    background-color: #2a2a2e;
}

.QFrame {
    background-color: #2a2a2e;
}

.QPushButton:hover {
    border: 2.3px solid blue;
    }

.QLineEdit#listValue {
    border: 1px solid #878290;
    border-radius: 5px;
    font-size: 12px;
    padding-left: 5px;
    color: black;
    }

QListWidget {
    background-color: #2a2a2e;
}

.QLineEdit#changeValue {
    color: black;
    border: 1px solid #878290;
    border-top-left-radius: 5px ;
    border-bottom-left-radius: 5px;
    }

.QListWidget {
    color: grey;
}

.QCheckBox {
    color: grey;
    font-size: 11.5 px;
    color: grey;
    }

.QCheckBox::indicator:checked {
    border : 4px solid rgb(91, 137, 75);
    border-radius: 6px;
}

.QCheckBox::indicator:unchecked {
    border : 4px solid rgb(159, 53, 53);
    border-radius: 6px;
}